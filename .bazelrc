build --strategy=Javac=worker

build --verbose_failures=false

build --define BUILD_NUMBER=developer
build --define RELEASE_VERSION=snapshot
build --define GIT_REVISION=working-tree
build --define BUILD_BY=`whoami`
build --define BUILD_AT=`'date +%Y-%m-%d-%H%M%S'`

test --test_output=errors

try-import user.bazelrc
